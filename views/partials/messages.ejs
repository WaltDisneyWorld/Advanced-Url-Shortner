<% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
    <div class="error">
        <%= error.msg %>
    </div>
    <% }); %> <% } %> <% if(success_msg != ''){ %>
    <div class="done">
        <%= success_msg %>
    </div>
    <% } %> <% if(error_msg != ''){ %>
    <div class="error">
        <%= error_msg %>
    </div>
    <% } %> <% if(error != ''){ %>
    <div class="done">
        <%= error %>
    </div>    
<% } %>